# Core service settings
FUALAB_SERVICE_NAME=FUALab
FUALAB_DATABASE_URL=postgresql://postgres:postgres@db:5432/fualab
FUALAB_REDIS_URL=redis://redis:6379/0
FUALAB_HEARTBEAT_INTERVAL_SECONDS=15

# Database container
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=fualab

# Redis container
REDIS_PORT=6379

# AWS placeholders for Terraform-lite
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=000000000000
FUALAB_ECR_REPO_API=fualab-api
FUALAB_ECR_REPO_WORKER=fualab-worker
FUALAB_ECS_CLUSTER=fualab-cluster
